%% -*- erlang -*-
{erl_opts, [debug_info]}.
{deps, [
        {meck, ".*", {git, "git://github.com/esl/meck.git", "HEAD"}}
       ]}.
{dialyzer_opts, [{warnings, [no_unused,
                             no_improper_lists, no_fun_app, no_match,
